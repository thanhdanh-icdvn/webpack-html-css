{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-trailing-whitespace": [true, "ignore-jsdoc"],
        "prefer-const": [true, {"destructuring": "any"}],
        "semicolon": [true, "always"],
        "space-before-function-paren": true,
        "quotemark": [true, "single"]
    },
    "rulesDirectory": []
}